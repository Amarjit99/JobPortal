  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --verbose",
    "test:security": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js security-tests/ --detectOpenHandles --forceExit",
    "test:security:injection": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js security-tests/injection.test.js",
    "test:security:xss": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js security-tests/xss.test.js",
    "test:security:access": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js security-tests/access-control.test.js",
    "test:performance": "artillery run performance-tests/load-test.yml --output performance-results/load-test.json",
    "test:benchmark": "artillery run performance-tests/benchmark.yml --output performance-results/benchmark.json",
    "test:stress": "artillery run performance-tests/stress-test.yml --output performance-results/stress-test.json",
    "test:spike": "artillery run performance-tests/spike-test.yml --output performance-results/spike-test.json",
    "perf:analyze": "node performance-tests/analyze-queries.js",
    "perf:indexes": "node performance-tests/create-indexes.js",
    "perf:report": "artillery report performance-results/load-test.json --output performance-results/load-test.html",
    "audit": "npm audit --production",
    "audit:fix": "npm audit fix"
  },
