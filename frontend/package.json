{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:browser": "playwright test browser-compatibility.spec.js",
    "test:browser:chrome": "playwright test browser-compatibility.spec.js --project=chromium",
    "test:browser:firefox": "playwright test browser-compatibility.spec.js --project=firefox",
    "test:browser:safari": "playwright test browser-compatibility.spec.js --project=webkit",
    "test:browser:edge": "playwright test browser-compatibility.spec.js --project='Microsoft Edge'",
    "test:browser:mobile": "playwright test browser-compatibility.spec.js --project='Mobile Chrome' --project='Mobile Safari'",
    "test:browser:all": "playwright test browser-compatibility.spec.js --project=chromium --project=firefox --project=webkit",
    "test:browser:headed": "playwright test browser-compatibility.spec.js --headed",
    "test:browser:report": "playwright test browser-compatibility.spec.js && playwright show-report"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.1.6",
    "framer-motion": "^11.3.7",
    "lucide-react": "^0.395.0",
    "next-themes": "^0.3.0",
    "pdfkit": "^0.17.2",
    "razorpay": "^2.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "recharts": "^3.6.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.3",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.11.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "lighthouse": "^12.8.2",
    "pa11y": "^9.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0"
  }
}
